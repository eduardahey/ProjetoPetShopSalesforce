        }
        return null;
    }

    public ContactMapper construirContactRecord(C3C_SObject record)
    {
        this.initBuild();
        this.record = record;

        this.recordId = record.getStringByField('Id');
        this.salutation = record.getStringByField('Salutation');
        this.firstName = record.getStringByField('FirstName');
        this.lastName = record.getStringByField('LastName');
        this.phone = record.getStringByField('Phone');
        this.homePhone = record.getStringByField('HomePhone');
        this.mobilePhone = record.getStringByField('MobilePhone');
        this.otherPhone = record.getStringByField('OtherPhone');
        this.email = record.getStringByField('Email');
        this.assistantName = record.getStringByField('AssistantName');
        this.assistantPhone = record.getStringByField('AssistantPhone');
        this.accountId = record.getStringByField('AccountId');
        this.title = record.getStringByField('Title');
        this.department = record.getStringByField('Department');
        this.birthdate = record.getDateByField('BirthDate');
        this.mailingStreet = record.getStringByField('MailingStreet');
        this.mailingCity = record.getStringByField('MailingCity');
        this.mailingState = record.getStringByField('MailingState');
        this.mailingCountry = record.getStringByField('MailingCountry');
        this.mailingPostalCode =  record.getStringByField('MailingPostalCode');
        this.otherStreet = record.getStringByField('OtherStreet');
        this.otherCity = record.getStringByField('OtherCity');
        this.otherState = record.getStringByField('OtherState');
        this.otherCountry = record.getStringByField('OtherCountry');
        this.otherPostalCode = record.getStringByField('OtherPostalCode');
        this.description = record.getStringByField('Description');
        this.cpf = record.getStringByField('Cpf__c');

        this.finishBuild();
        return this;
